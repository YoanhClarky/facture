         		FIN
         	FIN
         sinon
         	produits.designation = SAI_NOM_DU_PRODUIT
         	produits.Prix = SAI_PRIX_DU_PRODUIT
         	HAjoute(produits)
         	Info("Produit enregistrer")
         FIN
         TableAffiche(FEN_Tableau_de_bord.TABLE_Produits,taCourantEnreg)
         FEN_Tableau_de_bord.LIB_nbr_produit = produits.NbEnr()
         ferme()
